# PostgreSQL Configuration
POSTGRES_DB=rickandmorty_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PORT=6379

# WebApp Configuration
WEBAPP_PORT=8080
ASPNETCORE_ENVIRONMENT=Production

# Connection Strings (used by WebApp)
ConnectionStrings__PostgreSqlConnection=Host=postgres;Port=5432;Database=rickandmorty_db;Username=postgres;Password=postgres_password
ConnectionStrings__RedisConnection=redis:6379

# Cache Configuration
AppCache__AbsoluteExpirationRelativeToNow=00:05:00
AppCache__CompressionThresholdBytes=2048
